{
    "key": "jira-planning-calendar-app",
    "name": "Jira Planning Calendar",
    "description": "Jira Planning Calendar",
    "baseUrl": "{{localBaseUrl}}",
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [
        "READ"
    ],
    "modules": {
        "generalPages": [
            {
                "key": "jira-planning-calendar-page-jira",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "Jira Planning Calendar"
                },
                "url": "/jira-planning-calendar",
                "conditions": [{
                    "condition": "user_is_logged_in"
                }]
            }
        ]
    }
}
